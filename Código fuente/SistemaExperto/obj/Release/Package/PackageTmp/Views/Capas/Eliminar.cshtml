@model SistemaExperto.Models.Capas

@{
    ViewBag.Title = "Eliminar";
}
<!-- Estilos -->
<link href="~/Content/css/se.css" rel="stylesheet" />
<link href="~/Content/css/main.css" rel="stylesheet" />
<link href="~/Content/css/whhg.css" rel="stylesheet" />

<!-- Ruta de navegación -->
<ol class="breadcrumb">
    <li><a href="/">Inicio</a></li>
    <li><a href="~/Administracion/Index">Administración</a></li>
    <li><a href="~/Administracion/Mapa">Mapas</a></li>
    <li><a href="~/Capas/Index">Capas</a></li>
    <li class="active">Eliminar capa</li>
</ol>

<!-- Contenido -->
<div class="row col-md-10 col-md-offset-1 contenido">

    <!--Título-->
    <h2>Eliminar capa</h2>
    <hr />
    <!-- Datos -->
    <div>
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.NombreCaracterizacion)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.NombreCaracterizacion)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.IdentificadorCaracterizacion)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.IdentificadorCaracterizacion)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Concepto)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Concepto)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.RutaImagen)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.RutaImagen)
            </dd>

        </dl>

        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

          <!-- Botón eliminar -->
            <div class="form-group">
                <div class="col-md-12 text-center">
                    <br />
                    <input type="submit" value="Eliminar" class="btn btn-primary" />
                </div>
            </div>
        }
    </div>


</div>